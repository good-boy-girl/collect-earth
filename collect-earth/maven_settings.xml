<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <profiles>
    <profile>
		<id>assembly</id>
		<properties>
			<installbuilder-home>PATH TO THE INSTALLBUILDER ROOT FOLDER</installbuilder-home>
			<installbuilder-output-directory>PATH TO THE INSTALLBUILDER INSTALLERS OUTPUT FOLDER</installbuilder-output-directory>
			<planet_maps_ce_key>PLANET MAPS API KEY</planet_maps_ce_key>
		</properties>
	</profile>
  </profiles>
  <servers>
    <server>
      <id>openforis-releases</id>
      <username>NEXUS SERVER USERNAME</username>
      <password>NEXUS SERVER PASSWORD</password>
    </server>
	<server>
      <id>openforis-snapshots</id>
      <username>NEXUS SERVER USERNAME</username>
      <password>NEXUS SERVER PASSWORD</password>
    </server>
	<server>
      <id>openforis-3rd-party</id>
      <username>NEXUS SERVER USERNAME</username>
      <password>NEXUS SERVER PASSWORD</password>
    </server>
	<server>
	  <id>github.com</id>
	  <username>GITHUB USERNAME</username>
	  <password>GITHUB PASSWORD</password>
	</server>
  </servers>
</settings>